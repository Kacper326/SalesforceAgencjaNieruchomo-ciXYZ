<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_it_open_space_and_Rooms_fields</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISCHANGED(Is_it_open_space__c),
  Is_it_open_space__c,
  NOT(ISBLANK(Rooms__c))
)</errorConditionFormula>
    <errorDisplayField>Rooms__c</errorDisplayField>
    <errorMessage>Jeśli biuro jest typu &quot;Open space&quot; wtedy nie uzupełniamy pola Rooms.</errorMessage>
</ValidationRule>
